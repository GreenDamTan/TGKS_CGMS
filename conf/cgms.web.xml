<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" 
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
  <package namespace="/cgms" name="cgms" extends="struts-default">

    <!-- upload action -->
	<action name="uploadCardImage" class="com.moemao.tgks.cgms.upload.action.UploadAction" method="uploadCardImage">  
        <result name="success">/WEB-INF/jsp/cgms/upload/upload.jsp</result>
        <result name="error">/WEB-INF/jsp/common/error.jsp</result>
    </action>
    
    <!-- cgms_loveLive -->
    <action name="loveLiveManager" class="com.moemao.tgks.cgms.lovelive.action.LoveLiveAction" method="loveLiveManager">
        <result name="success">/WEB-INF/jsp/cgms/loveLive/loveLiveManager.jsp</result>
        <result name="error">/WEB-INF/jsp/common/error.jsp</result>
    </action>

    <action name="queryLoveLive" class="com.moemao.tgks.cgms.lovelive.action.LoveLiveAction" method="queryLoveLive">
        <result name="success">/WEB-INF/jsp/cgms/loveLive/loveLiveList.jsp</result>
        <result name="error">/WEB-INF/jsp/common/error.jsp</result>
    </action>

    <action name="editLoveLivePage" class="com.moemao.tgks.cgms.lovelive.action.LoveLiveAction" method="editLoveLivePage">
        <result name="success">/WEB-INF/jsp/cgms/loveLive/loveLiveEdit.jsp</result>
        <result name="error">/WEB-INF/jsp/common/error.jsp</result>
    </action>

    <action name="editLoveLive" class="com.moemao.tgks.cgms.lovelive.action.LoveLiveAction" method="editLoveLive">
        <result name="success">/WEB-INF/jsp/common/success.jsp</result>
        <result name="error">/WEB-INF/jsp/common/error.jsp</result>
    </action>

    <action name="deleteLoveLive" class="com.moemao.tgks.cgms.lovelive.action.LoveLiveAction" method="deleteLoveLive">
        <result name="success">/WEB-INF/jsp/common/success.jsp</result>
        <result name="error">/WEB-INF/jsp/common/error.jsp</result>
    </action>
    
  </package>
</struts>